{
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/(google[a-z0-9]+\\.html)",
      "destination": "/$1"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}